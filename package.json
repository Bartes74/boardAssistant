{
  "name": "boardmemberassistant-monorepo",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.12.0",
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "scripts": {
    "preinstall": "node tools/scripts/check-node-version.mjs",
    "postinstall": "pnpm run generate",
    "generate": "pnpm -r --if-present run generate",
    "lint": "pnpm -r --workspace-concurrency=1 --if-present run lint",
    "test": "pnpm -r --if-present run test",
    "build": "pnpm -r --if-present run build",
    "dev": "pnpm -r --parallel --if-present run dev",
    "format": "pnpm -r --if-present run format",
    "prepare": "husky install",
    "bootstrap": "pnpm install && pnpm run generate && pnpm run lint",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "vercel:dev": "vercel dev"
  },
  "devDependencies": {
    "husky": "^9.1.6"
  }
}
